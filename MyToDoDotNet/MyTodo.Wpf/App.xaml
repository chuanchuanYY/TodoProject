<prism:PrismApplication
    x:Class="MyTodo.Wpf.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MyTodo.Wpf"
    xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Styles/Themes/DarkTheme.xaml" />
                <ResourceDictionary Source="./Styles/TextBlock.xaml" />
                <ResourceDictionary Source="./Styles/CheckBox.xaml" />
                <ResourceDictionary Source="./Styles/Button.xaml" />
                <ResourceDictionary Source="./Styles/RadioButton.xaml" />
                <ResourceDictionary Source="./Styles/ToggleButton.xaml" />
                <ResourceDictionary Source="./Styles/ComboBox.xaml" />
                <ResourceDictionary Source="./Styles/TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
